{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/featured\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/now\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/subscribe\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/writing\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/writing(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}